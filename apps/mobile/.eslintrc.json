{
  "extends": ["expo", "../../.eslintrc.js"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "caughtErrors": "none",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "@typescript-eslint/no-empty-object-type": "off",
    "react/no-unescaped-entities": "off"
  },
  "overrides": [
    {
      "files": ["src/utils/logger.ts"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
